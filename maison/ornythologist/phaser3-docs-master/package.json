  "bugs": {
    "url": "https://github.com/photonstorm/phaser3-docs/issues"
  },
  "homepage": "https://github.com/photonstorm/phaser3-docs#readme",
  "devDependencies": {
    "better-sqlite3": "^4.0.3",
    "directory-tree": "^2.0.0",
    "doctrine": "^2.0.0",
    "extract-comments": "^0.10.1",
    "foodoc": "0.0.8",
    "fs-extra": "^4.0.2",
    "ink-docstrap": "^1.3.2",
    "jsdoc": "^3.5.5",
    "jsdoc-json": "^2.0.2",
    "jsdoc-to-markdown": "^4.0.1",
    "json-beautify": "^1.0.1",

{
  "name": "phaser3-docs",
  "version": "0.1.0",
  "description": "Phaser 3 Documentation Generator",
  "main": "src/index.js",
  "scripts": {
    "build": "node src/index.js",
    "gen": "jsdoc -c jsdoc.conf.json -R ../phaser/README.md",

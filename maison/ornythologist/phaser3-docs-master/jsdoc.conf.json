{
    "tags": {
        "allowUnknownTags": true
    },
    "markdown"  : {
        "parser"   : "gfm",
        "hardwrap" : true
    },
    "source": {
        "include": [
            "../phaser/src/"
        ],
        "gos": [
            "../phaser/src/gameobjects/index.js",
            "../phaser/src/gameobjects/components/",
            "../phaser/src/gameobjects/GameObject.js",
            "../phaser/src/gameobjects/GameObjectCreator.js",
            "../phaser/src/gameobjects/GameObjectFactory.js",
            "../phaser/src/gameobjects/DisplayList.js",
            "../phaser/src/gameobjects/UpdateList.js",
            "../phaser/src/gameobjects/BuildGameObject.js",
            "../phaser/src/gameobjects/BuildGameObjectAnimation.js",
            "../phaser/src/gameobjects/bitmaptext/",
            "../phaser/src/gameobjects/blitter/",
            "../phaser/src/gameobjects/graphics/",
            "../phaser/src/gameobjects/group/",
            "../phaser/src/gameobjects/image/",
            "../phaser/src/gameobjects/lights/",
            "../phaser/src/gameobjects/mesh/",
            "../phaser/src/gameobjects/particles/",
            "../phaser/src/gameobjects/pathfollower/",
            "../phaser/src/gameobjects/quad/",
            "../phaser/src/gameobjects/sprite/",
            "../phaser/src/gameobjects/sprite3d/",
            "../phaser/src/gameobjects/text/",
            "../phaser/src/gameobjects/tilesprite/",
            "../phaser/src/gameobjects/zone/"
        ],
        "done": [
            "../phaser/src/actions/",
            "../phaser/src/animations/",
            "../phaser/src/boot/",
            "../phaser/src/cache/",
            "../phaser/src/cameras/",
            "../phaser/src/create/",
            "../phaser/src/curves/",
            "../phaser/src/data/",
            "../phaser/src/device/",
            "../phaser/src/display/",
            "../phaser/src/dom/",
            "../phaser/src/events/",
            "../phaser/src/gameobjects/",
            "../phaser/src/geom/",
            "../phaser/src/input/",
            "../phaser/src/loader/",
            "../phaser/src/math/",
            "../phaser/src/physics/",
            "../phaser/src/renderer/",
            "../phaser/src/scene/",
            "../phaser/src/sound/",
            "../phaser/src/structs/",
            "../phaser/src/textures/",
            "../phaser/src/tilemaps/",
            "../phaser/src/time/",
            "../phaser/src/tweens/",
            "../phaser/src/utils/"
        ],
        "exclude": [
            "../phaser/src/physics/matter-js/lib",
            "../phaser/src/polyfills"
        ],
        "includePattern": ".+\\.js?$",
        "excludePattern": "(^|\\/|\\\\)_"
    },
    "plugins": [
        "jsdoc-plugins/typedef"
    ],
    "templates": {
        "systemName": "Phaser",
        "systemSummary": "HTML5 Game Framework",
        "systemColor": "#0f0019",
        "footer": "Spotted an error in the docs? Please open an issue!",
        "copyright": "Phaser Copyright Â© 2018 Photon Storm Ltd and the contributors to the Phaser projects.",
        "inlineNav": true,
        "includeDate": true,
        "collapseSymbols": false,
        "methodHeadingReturns": false,
        "sort": "longname, version, since",
        "theme": "cerulean"
    },
    "opts": {
        "debug": true,
        "destination": "./out/",
        "encoding": "utf8",
        "outputSourceFiles": false,
        "outputSourcePath": true,
        "recurse": true,
        "private": false,
        "lenient": true,
        "sourceType": "script",
        "template": "node_modules/ink-docstrap/template",
        "_template": "node_modules/foodoc/template"
    }
}